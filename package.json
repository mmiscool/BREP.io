{
  "name": "brep-io-kernel",
  "version": "1.0.0",
  "scripts": {
    "dev": "pnpm generateLicenses && pnpm build:kernel && vite --host 0.0.0.0",
    "build": "pnpm generateLicenses && vite build",
    "build:kernel": "pnpm generateLicenseBundle && vite build --config vite.config.kernel.js",
    "prepublishOnly": "pnpm build:kernel",
    "start": "pnpm dev",
    "liveTesting": "nodemon --watch src --watch tests --ext js --exec \"pnpm run test\"",
    "test": "node ./src/tests/tests.js",
    "debug:boolean": "node ./src/tests/debugBoolean.js",
    "generateLicenses": "node ./generateLicenses.js",
    "generateLicenseBundle": "node ./scripts/generateLicenseBundle.js",
    "capture": "node ./scripts/capture.js & pnpm dev "
  },
  "type": "module",
  "main": "./dist-kernel/brep-kernel.js",
  "module": "./dist-kernel/brep-kernel.js",
  "exports": {
    ".": "./dist-kernel/brep-kernel.js",
    "./BREP": "./dist-kernel/brep-kernel.js",
    "./PartHistory": "./dist-kernel/brep-kernel.js",
    "./AssemblyConstraintHistory": "./dist-kernel/brep-kernel.js",
    "./AssemblyConstraintRegistry": "./dist-kernel/brep-kernel.js",
    "./SketchSolver2D": "./src/SketchSolver2D.js"
  },
  "files": [
    "src/**",
    "dist-kernel/**",
    "README.md",
    "LICENSE.md"
  ],
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@tarikjabiri/dxf": "^2.8.9",
    "jszip": "^3.10.1",
    "manifold-3d": "^3.3.2",
    "monaco-editor": "^0.55.1",
    "tess2": "^1.0.0",
    "three": "^0.182.0",
    "three-mesh-bvh": "^0.9.3"
  },
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmiscool/BREP.io"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "playwright": "^1.57.0",
    "vite": "^7.2.7",
    "vite-plugin-wasm": "^3.5.0"
  }
}

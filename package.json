  "license": "See licence.md",
  "dependencies": {
    "@tarikjabiri/dxf": "^2.8.9",
    "jszip": "^3.10.1",
    "manifold-3d": "^3.3.2",
    "monaco-editor": "^0.55.1",
    "tess2": "^1.0.0",
    "three": "^0.182.0",
    "three-mesh-bvh": "^0.9.5"
  },
  "description": "",
  "devDependencies": {
    "nodemon": "^3.1.11",
    "playwright": "^1.57.0",
    "vite": "^7.3.1",
    "vite-plugin-wasm": "^3.5.0"
  }
